{
    # 通用配置版本
    "version": "2.5.1",

    # 监听与开奖来源（所有账号默认共用，可在账号配置中覆盖）
    "groups": {
        "monitor": [
            "me"
        ],
        "zq_group": [
            "me"
        ],
        "zq_bot": 0
    },

    # 朱雀公共接口（每个账号自己的 cookie/x_csrf 放到账号配置）
    "zhuque": {
        "api_url": "https://zhuque.in/api/user/getInfo?"
    },

    # 更新配置（私有仓库才需要）
    "update": {
        "github_token": ""
    }
}
